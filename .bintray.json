    {
      "includePattern": "(.*_i386\\.deb)",
      "uploadPattern": "$1",
      "matrixParams": {
        "deb_distribution": "precise,trusty,vivid",
        "deb_component": "main",
        "deb_architecture": "i386"}
    }
  ]
}
